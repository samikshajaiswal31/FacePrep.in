/*
import java.util.Scanner;
class Main
{
   public static void main(String args[])
   {
   	Scanner in = new Scanner(System.in);
	int n = in.nextInt(),count = 0;                    //  n = 4
	for(int row = 1 ; row <= n ; row++)
	{
	   for(int col = 1 ; col <= row ; col++)
	   {
	      System.out.print(++count+" ");              // o/p : 1
